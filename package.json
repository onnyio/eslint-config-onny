    "Isaac Marotte <isaac@onny.io> (http://onny.io)"
  ],
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/onnyio/eslint-config-onny/issues"
  },
  "homepage": "https://github.com/onnyio/eslint-config-onny#readme",
  "dependencies": {
    "chalk": "^4.1.2",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-import-resolver-webpack": "^0.13.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-promise": "^4.3.1",
    "strip-ansi": "^6.0.0"
